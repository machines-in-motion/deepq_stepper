<?xml version="1.0" encoding="utf-8"?>

<robot name="stairs">
  <link name="ground_link">
    <contact>
      <lateral_friction value="5.0"/>
      <restitution value="0.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <geometry>
        <box size=".6 1.0 .05"/>      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <geometry>
        <box size=".6 1.0 .05"/>
      </geometry>
    </collision>
  </link>

  <joint name="ground_to_first_step" type="fixed">
      <parent link="ground_link"/>
      <child link="first_step_link"/>
   </joint>


  <link name="first_step_link">
    <contact>
      <lateral_friction value="10.0"/>
      <restitution value="0.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.5 0 0.025"/>
      <geometry>
        <box size="0.2 1.0 .05"/>      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.6 0 0.025"/>
      <geometry>
        <box size="0.4 1.0 .05"/>
      </geometry>
    </collision>
  </link>

  <link name="second_step_link">
    <contact>
      <lateral_friction value="10"/>
      <restitution value="0.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz=".8 .3 0.025"/>
      <geometry>
        <box size="0.2 .5 .05"/>      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.8 .3 0.025"/>
      <geometry>
        <box size="0.2 .5 .05"/>
      </geometry>
    </collision>
  </link>

   <joint name="first_to_second_step" type="fixed">
      <parent link="first_step_link"/>
      <child link="second_step_link"/>
      <origin xyz="0 0 0 "/>
   </joint>

  <link name="third_step_link">
    <contact>
      <lateral_friction value="10.0"/>
      <restitution value="0.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0.8 -.3 0.025"/>
      <mass value=".0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.8 -.3 0.025"/>
      <geometry>
        <box size="0.2 0.5 .05"/>      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.8 0 0.025"/>
      <geometry>
        <box size="0.2 0.5 .05"/>
      </geometry>
    </collision>
  </link>
 
 <joint name="second_to_third_step" type="fixed">
      <parent link="second_step_link"/>
      <child link="third_step_link"/>
      <origin xyz="0 0 0 "/>
  </joint>


</robot>